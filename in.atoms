variable	nTC    equal   ${nC}*${nChains}
variable	nTD    equal   ${nD}*${nChains}
variable	nTE    equal   ${nE}*${nChains}

variable	nTot	equal	${nTA}+${nTB}+${nTC}+${nTD}+${nTE}



